<form action="/objectives/new-objective" method="POST">
  <input type="text" name="problem" placeholder="Describe the problem you want to solve">
  <select name="category" id="">
    <option name="Financial">Financial</option>
    <option name="Career">Career or Passion</option>
    <option name="Relationship">Relationship</option>
    <option name="Wellbeing">Wellbeing</option>
    <option name="Passion">Passion</option>

  </select>

  <input type="text" name="objectiveInput" placeholder="Describe the change you want to see">
  <input type="text" name="keyResult" placeholder="Provide one key results ">
  <input type="date" name="objectiveEndDate">
  <input type="date" name="objectiveEndDate" value="2021-06-01">

  <button type="submit">Submit</button>

</form>